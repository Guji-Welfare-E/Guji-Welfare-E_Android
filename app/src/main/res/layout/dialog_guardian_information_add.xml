<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="wrap_content"    android:layout_height="wrap_content"    android:paddingTop="10dp"    android:paddingBottom="15dp"    android:paddingEnd="30dp"    android:paddingStart="30dp"    android:background="@drawable/background_dialog">    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/appCompatTextView16"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/pretendard_bold"        android:text="보호자 등록"        android:textColor="#00BE1E"        android:textSize="17sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <androidx.appcompat.widget.AppCompatEditText        android:id="@+id/guardian_name_text"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:background="@drawable/background_edittext"        android:fontFamily="@font/pretendard_bold"        android:gravity="center"        android:hint="보호자 이름"        android:maxLines="1"        android:textSize="20sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/appCompatTextView16" />    <androidx.appcompat.widget.AppCompatEditText        android:id="@+id/guardian_phone_number_text"        android:layout_width="220dp"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:background="@drawable/background_edittext"        android:fontFamily="@font/pretendard_bold"        android:gravity="center"        android:hint="보호자 전화번호"        android:maxLines="1"        android:textSize="20sp"        app:layout_constraintStart_toStartOf="@+id/guardian_name_text"        app:layout_constraintTop_toBottomOf="@+id/guardian_name_text" />    <androidx.appcompat.widget.AppCompatButton        android:id="@+id/find_phone_number_button"        android:layout_width="60dp"        android:layout_height="40dp"        android:background="@drawable/background_edittext"        android:gravity="center"        android:hint="번호 찾기"        android:maxLines="1"        android:textSize="12sp"        android:backgroundTint="#BFFF9C"        android:fontFamily="@font/pretendard_bold"        app:layout_constraintBottom_toBottomOf="@+id/guardian_phone_number_text"        app:layout_constraintEnd_toEndOf="@+id/guardian_name_text"        app:layout_constraintTop_toTopOf="@+id/guardian_phone_number_text" />    <androidx.appcompat.widget.AppCompatEditText        android:id="@+id/guardian_relationship_text"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:background="@drawable/background_edittext"        android:fontFamily="@font/pretendard_bold"        android:gravity="center"        android:hint="보호자 관계"        android:layout_marginTop="10dp"        android:maxLines="1"        android:textSize="20sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/guardian_phone_number_text" />    <androidx.appcompat.widget.AppCompatButton        android:id="@+id/guardian_add_button"        android:layout_width="170dp"        android:layout_height="30dp"        android:background="@drawable/background_dialog_button"        android:backgroundTint="#C2C2C2"        android:fontFamily="@font/pretendard_bold"        android:text="보호자 추가"        android:textColor="@color/white"        android:textSize="13sp"        android:layout_marginTop="20dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/guardian_relationship_text" /></androidx.constraintlayout.widget.ConstraintLayout>